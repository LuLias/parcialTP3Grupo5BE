<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.ParcialTP3Grupo5BE" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        Theme.AppCompat.DayNight
        <!--  parent="Theme.Material3.DayNight.NoActionBar"  </item> -->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:navigationBarColor">
            @color/white
        </item>

        <item name="android:statusBarColor">
            @color/white
        </item>

        <item name="android:windowLightStatusBar">
            @color/white
        </item>
    </style>

    <style name="Theme.ParcialTP3Grupo5BE" parent="Base.Theme.ParcialTP3Grupo5BE" />


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="AppTheme.ResultsAppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="AppTheme.TextBottomNavActive" parent="Widget.Material3.BottomNavigationView" >
        <item name="color">@color/blue</item>
    </style>



    <declare-styleable name="CustomSegmentedButton">
        <attr name="checkedButton" format="reference" />
    </declare-styleable>



</resources>